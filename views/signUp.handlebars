<h1>Registrar usuario</h1>
<form action="/signup" method="post" id="formLogIn">
    <div class="mb-3">
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="email" name="username">
    </div>
    <div class="mb-3">
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="password" name="password">
    </div>
    <div class="mb-3">
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="nombre" name="name">
    </div>
    <div class="mb-3">
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="dirección" name="adress">
    </div>
    <div class="mb-3">
        <input type="text" class="form-control" id="formGroupExampleInput" type="tel" placeholder="teléfono" name="number">
    </div>
    <div class="form-check">
        <input class="form-check-input" type="checkbox" value="true" name="isAdmin" id="flexCheckDefault">
        <label class="form-check-label" for="flexCheckDefault">
            Admin
        </label>
    </div>
    <button class="btn btn-dark">Enviar</button>
</form>
<a class="btn btn-primary" href="/" role="button">Log In</a>